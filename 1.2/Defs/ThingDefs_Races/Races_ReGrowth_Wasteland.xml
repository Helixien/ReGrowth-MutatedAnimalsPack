<Defs>

    <!-- ======================= Wasteland Deer (Rimstag) =============================== -->
    <ThingDef ParentName="AnimalThingBase">
        <defName>RG-WF_WastelandDeer</defName>
        <label>Rimstag</label>
        <description>A medium-sized herding herbivore changed and mutated by generations of exosure to toxic waste which prefers to live in mixed forests and plains. Rimstag are very quick and have two heads.</description>
        <statBases>
            <MoveSpeed>5.3</MoveSpeed>
            <MarketValue>145</MarketValue>
            <ComfyTemperatureMin>-35</ComfyTemperatureMin>
            <ComfyTemperatureMax>25</ComfyTemperatureMax>
			<ToxicSensitivity>0</ToxicSensitivity>
        </statBases>
        <tools>
            <li>
                <label>left hoof</label>
                <capacities>
                    <li>Blunt</li>
                    <li>Poke</li>
                </capacities>
                <power>7</power>
                <cooldownTime>2</cooldownTime>
                <linkedBodyPartsGroup>FrontLeftLeg</linkedBodyPartsGroup>
            </li>
            <li>
                <label>right hoof</label>
                <capacities>
                    <li>Blunt</li>
                    <li>Poke</li>
                </capacities>
                <power>7</power>
                <cooldownTime>2</cooldownTime>
                <linkedBodyPartsGroup>FrontRightLeg</linkedBodyPartsGroup>
            </li>
            <li>
                <label>teeth</label>
                <capacities>
                    <li>Bite</li>
                </capacities>
                <power>8</power>
                <cooldownTime>2</cooldownTime>
                <linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
                <chanceFactor>0.5</chanceFactor>
            </li>
            <li>
                <label>left head</label>
                <capacities>
                    <li>Blunt</li>
                </capacities>
                <power>4</power>
                <cooldownTime>2</cooldownTime>
                <linkedBodyPartsGroup>RG-WF_RightHeadAttackTool</linkedBodyPartsGroup>
                <ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
                <chanceFactor>0.2</chanceFactor>
            </li>
            <li>
                <label>right head</label>
                <capacities>
                    <li>Blunt</li>
                </capacities>
                <power>6</power>
                <cooldownTime>2</cooldownTime>
                <linkedBodyPartsGroup>RG-WF_LeftHeadAttackTool</linkedBodyPartsGroup>
                <ensureLinkedBodyPartsGroupAlwaysUsable>true</ensureLinkedBodyPartsGroupAlwaysUsable>
                <chanceFactor>0.2</chanceFactor>
            </li>
        </tools>
        <race>
            <body>RG-WF_QuadrupedAnimalWithHooves</body>
            <herdAnimal>true</herdAnimal>
            <baseBodySize>0.9</baseBodySize>
            <baseHealthScale>0.9</baseHealthScale>
            <baseHungerRate>0.40</baseHungerRate>
            <foodType>VegetarianRoughAnimal</foodType>
            <leatherDef>Leather_Plain</leatherDef>
            <meatLabel>venison</meatLabel>
            <wildness>0.75</wildness>
            <nuzzleMtbHours>60</nuzzleMtbHours>
            <gestationPeriodDays>24</gestationPeriodDays>
            <lifeExpectancy>25</lifeExpectancy>
            <manhunterOnDamageChance>0.05</manhunterOnDamageChance>
            <manhunterOnDamageChance>0.05</manhunterOnDamageChance>
            <lifeStageAges>
                <li>
                    <def>AnimalBaby</def>
                    <minAge>0</minAge>
                </li>
                <li>
                    <def>AnimalJuvenile</def>
                    <minAge>0.2</minAge>
                </li>
                <li>
                    <def>AnimalAdult</def>
                    <minAge>0.5</minAge>
                    <soundWounded>Pawn_Deer_Wounded</soundWounded>
                    <soundDeath>Pawn_Deer_Death</soundDeath>
                    <soundCall>Pawn_Deer_Call</soundCall>
                    <soundAngry>Pawn_Deer_Angry</soundAngry>
                </li>
            </lifeStageAges>
            <soundMeleeHitPawn>Pawn_Melee_BigBash_HitPawn</soundMeleeHitPawn>
            <soundMeleeHitBuilding>Pawn_Melee_BigBash_HitBuilding</soundMeleeHitBuilding>
            <soundMeleeMiss>Pawn_Melee_BigBash_Miss</soundMeleeMiss>
        </race>
        <tradeTags>
            <li>AnimalUncommon</li>
            <li>RG-WF_AnimalWasteland</li>
        </tradeTags>
    </ThingDef>

